<!DOCTYPE html>
<%#Page trace="no"%>

<html>
<head>
   <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1"/>
   <script type="text/javascript" src="scripts.js"></script>
   <link rel="stylesheet" type="text/css" href="style.css">
   @Render Head
</head>

<body>
 <%#Code locality="page-global"%>
 <%
    #include "C:\inetpub\wwwroot\czynsze\HTMLWriter.ch"
 %>

  <!--- Page content goes here --->
   <%
      IF ::Session: uzytkownik!=nil
         ? HTMLWriter(): a("Budynki.cxp", "Budynki", "menu")
         ? HTMLWriter(): a("Lokale.cxp", "Lokale", "menu")
         ? HTMLWriter(): a("Najemcy.cxp", "Najemcy", "menu")
         ? HTMLWriter(): a("SkladnikiCzynszu.cxp", "Sk"+HTMLWriter(): _l()+"adniki czynszu", "menu")
   %>
   <div style="text-align: right; position: absolute; top: 10px; right: 10px;">
   <%
      ? "U"+HTMLWriter(): _z()+"ytkownik: "+::Session: uzytkownik+" "+HTMLWriter(): a("UserValidation.cxp", "Wyloguj")
   %>
   </div>
   <hr>

   @Render Body

   <%
      ELSE
         ::HttpResponse: Redirect("Login.cxp")
      ENDIF
   %>
</body>

</html>